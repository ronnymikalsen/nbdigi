{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"],
        "urls": [
          "https://fonts.googleapis.com/css?family=Roboto:300,400,500|Roboto+Mono:300",
          "https://fonts.googleapis.com/icon?family=Material+Icons"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "https://api.nb.no/catalog/v1/search?q=-pleasecacheme**",
        "https://api.nb.no/catalog/v1/iiif/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 20,
        "maxAge": "2h"
      }
    },
    {
      "name": "thumbnails",
      "urls": ["https://www.nb.no/services/image/resolver/**/245,245/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 1000,
        "maxAge": "7d"
      }
    }
  ]
}
