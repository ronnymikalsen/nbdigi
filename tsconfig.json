{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@nbdigi/ui-material": [
        "libs/ui-material/src/index.ts"
      ],
      "@nbdigi/home-view": [
        "libs/home-view/src/index.ts"
      ],
      "@nbdigi/explore-view": [
        "libs/explore-view/src/index.ts"
      ],
      "@nbdigi/search-view": [
        "libs/search-view/src/index.ts"
      ],
      "@nbdigi/my-library-view": [
        "libs/my-library-view/src/index.ts"
      ],
      "@nbdigi/ui-my-settings": [
        "libs/ui-my-settings/src/index.ts"
      ],
      "@nbdigi/settings-view": [
        "libs/settings-view/src/index.ts"
      ],
      "@nbdigi/backend": [
        "libs/backend/src/index.ts"
      ],
      "@nbdigi/data-models": [
        "libs/data-models/src/index.ts"
      ],
      "@nbdigi/session-state": [
        "libs/session-state/src/index.ts"
      ],
      "@nbdigi/ui-common": [
        "libs/ui-common/src/index.ts"
      ],
      "@nbdigi/item-state": [
        "libs/item-state/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
