    "test:once": "ng test --single-run",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint": "ng lint",
    "coverage": "ng test --single-run --code-coverage",
    "e2e": "ng e2e",
    "deploy": "firebase deploy"
  },
  "private": true,
