    "@ngrx/store-devtools": "7.4.0",
    "@nrwl/builders": "7.8.3",
    "@nrwl/schematics": "7.8.3",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "24.0.9",
    "@types/node": "~10.12.10",
    "codelyzer": "~4.5.0",
    "firebase-tools": "^6.1.2",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.1.0",
